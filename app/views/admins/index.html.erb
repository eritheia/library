<%= link_to "Add Admin", new_admin_path %>
<table class="table table-bordered">
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th></th>
		<th></th>
	</thead>
	<tbody>
		<% @admins.each do |admin| %>
			<tr>
				<td><%= admin.first_name %></td>
				<td><%= admin.last_name %></td>
				<td><%= admin.email %></td>
				<td><%= link_to "Delete", "/admins/#{admin.id}", method: :delete %></td>
				<td><%= link_to "Show", "/admins/#{admin.id}", method: :get %></td>

			</tr>
		<% end %>
	</tbody>
</table>
